{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "email",
    "name",
    "surname"
  ],
  "properties": {
    "id": {
      "type": "string",
      "minLength": 5,
      "maxLength": 100
    },
    "name": {
      "type": "string",
      "minLength": 5,
      "maxLength": 100
    },
    "surname": {
      "type": "string",
      "minLength": 5,
      "maxLength": 100
    }
  }
}